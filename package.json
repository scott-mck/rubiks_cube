{
  "name": "rubiks_cube",
  "version": "1.0.0",
  "description": "A Rubik's Cube Browser Simulator",
  "main": "rubiks_cube.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm-run-all clean js:ugly css:ugly",
    "watch": "npm-run-all -p css:watch js:watch",
    "clean": "rm -rf dist/*",
    "css": "node-sass src/scss/main.scss dist/built.css",
    "css:ugly": "node-sass src/scss/main.scss dist/built.css --output-style=compressed",
    "css:watch": "onchange -i 'src/scss/**/*.scss' -- npm run css",
    "js": "babel-node scripts/js-start.js && rollup -c && babel-node scripts/js-end.js",
    "js:ugly": "rollup -c && uglifyjs dist/built.js -mc -o dist/built.js",
    "js:watch": "onchange -i 'src/js/**/*.js' -- npm run js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/scott-mck/rubiks_cube.git"
  },
  "author": "Scott McKenzie",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/scott-mck/rubiks_cube/issues"
  },
  "homepage": "https://github.com/scott-mck/rubiks_cube#readme",
  "dependencies": {
    "gsap": "^1.19.0",
    "jquery": "^3.1.0",
    "three": "^0.79.0"
  },
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-es2015-rollup": "^1.2.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.0",
    "chalk": "^1.1.3",
    "del": "^2.2.1",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-clean-css": "^2.0.12",
    "gulp-concat": "^2.6.0",
    "gulp-plumber": "^1.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-rollup": "^2.5.0",
    "gulp-sass": "^2.3.2",
    "gulp-shell": "^0.5.2",
    "gulp-uglify": "^1.5.4",
    "gulp-watch": "^4.3.9",
    "node-babel": "^0.1.2",
    "node-sass": "^3.10.0",
    "npm-run-all": "^3.1.0",
    "onchange": "^3.0.2",
    "rollup": "^0.36.0",
    "rollup-plugin-babel": "^2.6.1",
    "rollup-plugin-commonjs": "^5.0.4",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-uglify": "^1.0.1",
    "uglify-js": "^2.7.3",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0"
  }
}
